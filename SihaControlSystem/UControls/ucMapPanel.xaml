<UserControl x:Class="SihaControlSystem.UControls.ucMapPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SihaControlSystem.UControls"    xmlns:gmap="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
             mc:Ignorable="d" 
             d:DesignHeight="530" d:DesignWidth="680">
    <Grid x:Name="mainGrid">

        <Border Grid.Column="1" Background="Gray" Margin="5" >
            <gmap:GMapControl Name="gmapControl"
              Margin="5"
              Loaded="gmapControl_Loaded"/>
        </Border>

    </Grid>
</UserControl>
